{
    "AWSEBDockerrunVersion": "2",
    "family": "aws-lamp-app",
    "volumes": [
        {
            "name": "lamp-app-vol",
            "host": {}
        }
    ],
    "containerDefinitions": [
        {
            "environment": [],
            "name": "lamp-app",
            "image": "goffinator/aws-lamp-app",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 80
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "lamp-app-vol",
                    "containerPath": "/var/www/lamp-app-vol"
                }
            ],
            "essential": true
        }
    ]
}
